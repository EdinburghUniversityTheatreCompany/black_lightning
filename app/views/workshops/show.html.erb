<%= render '/events/basic_info', event: @workshop, event_type: 'workshop' %>

<%= render '/events/publicity_text_and_team_members', event: @workshop %>

<%= render '/admin/events/reviews', reviews: @workshop.reviews.accessible_by(current_ability) %>

<%= render '/shared/gallery', pictures: @workshop.pictures.accessible_by(current_ability, :show) %>

<%= render 'shared/video_link_gallery', video_links: @workshop.video_links.accessible_by(current_ability, :show) %>

<%= render '/shared/show_actions', object: @workshop, index_link_params: { link_target: archives_workshops_path } %>
