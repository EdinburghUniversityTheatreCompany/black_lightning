<% headers = [:date] %>

<% field_sets = @maintenance_sessions.map { |maintenance_session| {
  fields: [
    maintenance_session
  ] } } %>

<% search_fields = {
  date_cont:            { slug: 'defaults.date' },
  users_full_name_cont: { slug: 'defaults.user' },
} %>

<%= render 'shared/pages/index', resource_class: MaintenanceSession, resources: @maintenance_sessions, headers: headers, field_sets: field_sets, search_fields: search_fields %>
